{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "amd": true
  },
  "rules": {
    "no-mixed-requires": false,
    "key-spacing": [2, { align: "value" }],
    "no-multi-spaces": false,
    "no-underscore-dangle": false,
    "no-use-before-define": "nofunc",
    "quotes": "single"
  }
}